% layout 'bootstrap';
% content_for 'head' => begin
  %= asset 'codemirror.js'
  %= asset 'codemirror.css'
% end

% title 'OBS synchronization Logs';
<H2><%= 'OBS synchronization Logs: ' . $folder %></H2>
<H6><%= $full %></H6>
% for my $subfolder (@$subfolders) {
	<label><%= link_to "$subfolder"  => url_for('plugin_obs_rsync_logfiles', folder => $folder, subfolder => $subfolder) %></label>
	<label>|</label>
% }
