% layout 'bootstrap';
% content_for 'head' => begin
  %= asset 'codemirror.js'
  %= asset 'codemirror.css'
% end

% title 'OBS synchronization Log';
<H2><%= 'OBS synchronization Log: ' . $folder . '/' . $subfolder %></H2>
<H6><%= $full %></H6>
% for my $filename (@$files) {
	<label><%= link_to "$filename"  => url_for('plugin_obs_rsync_download_file', folder => $folder, subfolder => $subfolder, filename => $filename) %></label>
	<label>|</label>
% }
